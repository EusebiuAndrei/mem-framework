{
  "name": "@eusebiu_gagea/mem",
  "version": "1.2.1",
  "description": "A scalable framework based on EventEmitter. It is build on top of expressjs using typescript.",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "start": "npm run build && npm run serve",
    "serve": "node -r dotenv/config build/server.js",
    "build": "npm run build-ts",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "watch-node": "nodemon -r dotenv/config build/server.js",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "eslint": "eslint . --ext .js,.ts",
    "test": "jest --forceExit --detectOpenHandles --coverage --verbose",
    "gen-docs": "npx typedoc --out ./docs --entryPointStrategy expand ./src/packages",
    "gen-docs-watch": "npx typedoc --watch --out ./docs --entryPointStrategy expand ./src/packages",
    "ts:start": "npx ts-node src/app/index.ts",
    "ts:watch": "npx ts-node-dev src/app/index.ts",
    "pck:publish": "npx tsc && npm publish --access public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EusebiuAndrei/framework.git"
  },
  "author": "EusebiuAndrei",
  "license": "Apache-2.0",
  "dependencies": {
    "@feathersjs/hooks": "^0.6.2",
    "body-parser": "^1.19.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.14.0",
    "concurrently": "^5.3.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "immer": "^9.0.6",
    "inversify": "^5.0.5",
    "inversify-inject-decorators": "^3.1.0",
    "lodash": "^4.17.21",
    "reflect-metadata": "^0.1.13",
    "shallow-equal-object": "^1.1.1",
    "ts-node-dev": "^1.1.8",
    "typedoc": "^0.22.6",
    "typeorm": "^0.2.38",
    "uuidv4": "^6.2.6",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^4.4.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.1",
    "@types/body-parser": "^1.19.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.3",
    "@types/hapi__joi": "^16.0.12",
    "@types/helmet": "0.0.45",
    "@types/jest": "^25.1.4",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/lodash": "^4.14.168",
    "@types/mongoose": "^5.7.7",
    "@types/morgan": "^1.9.0",
    "@types/node": "^13.9.3",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "colors": "^1.4.0",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "jest": "^25.2.1",
    "nodemon": "^2.0.13",
    "prettier": "^2.0.5",
    "supertest": "^4.0.2",
    "ts-jest": "^26.1.4",
    "ts-node": "^8.10.2",
    "typescript": "^3.8.3"
  },
  "bugs": {
    "url": "https://github.com/EusebiuAndrei/framework/issues"
  },
  "homepage": "https://github.com/EusebiuAndrei/framework#readme",
  "directories": {
    "doc": "docs"
  }
}
